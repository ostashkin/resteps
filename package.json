    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.2",
